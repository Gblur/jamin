<div class="container">
  <div class="row mx-3 mb-5">
    <%= render 'shared/search-result' %>
  </div>
  <div class="row">
    <div class="col-lg-8 col-sm-12">
        <%= render 'shared/card-s' %>
    </div>
    <div class="col-lg-4 pr-0">
      <div
        id="map"
        style="height: 100vh; position: sticky; top: 0px; border-radius: 6px; "
        data-markers="<%= @markers.to_json %>"
        data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>"
      ></div>
    </div>
  </div>
</div>
